  simulacro_carabina                      Int
  simulacro_espingarda                    Int
  simulacro_fuzil                         Int
  simulacro_garrucha                      Int
  simulacro_garruchao                     Int
  simulacro_metralhadora                  Int
  simulacro_outros                        Int
  simulacro_paintball                     Int
  simulacro_pistola                       Int
  simulacro_revolver                      Int
  simulacro_submetralhadora               Int
  simulacro_total                         Int
