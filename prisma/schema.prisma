generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model arearegiao {
  id             Int      @id @default(autoincrement())
  cisp           String
  aisp           String
  risp           String
  area_cisp_km2  Float
  area_aisp_km2  Float
  area_risp_km2  Float
}

model armaapreendida {
  id                                      Int      @id @default(autoincrement())
  cisp                                    String
  mes                                     Int
  ano                                     Int
  aisp                                    String
  risp                                    String
  arma_fogo_arma_fabricacao_caseira       Int
  arma_fogo_carabina                      Int
  arma_fogo_espingarda                    Int
  arma_fogo_fuzil                         Int
  arma_fogo_garrucha                      Int
  arma_fogo_garruchao                     Int
  arma_fogo_metralhadora                  Int
  arma_fogo_outros                        Int
  arma_fogo_pistola                       Int
  arma_fogo_revolver                      Int
  arma_fogo_submetralhadora               Int
  arma_fogo_total                         Int
  arma_branca_total                       Int
  artefato_explosivo_armadilha_explosiva  Int
  artefato_explosivo_armadilha_incendiaria Int
  artefato_explosivo_bomba_fabricacao_caseira Int
  artefato_explosivo_granada              Int
  artefato_explosivo_material_belico_explosivo Int
  artefato_explosivo_material_explosivo   Int
  artefato_explosivo_material_explosivo_caseiro Int
  artefato_explosivo_material_nao_identificado Int
